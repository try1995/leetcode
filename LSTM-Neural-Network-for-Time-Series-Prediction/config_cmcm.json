{
	"data": {
		"filename": "my.csv",
		"columns": ["Close", "Volume", "Money","ACCER", "ADTM", "MAADTM", "CCI", "DKX", "MADKX","MFI", "MTM", "ROC", "OSC", "MAOSC", "UDL", "MAUDL", "WR", "MAWR"],
		"sequence_length": 21,
		"normalise": true,
		"drop": true
	},
	"training": {
		"epochs": 2,
		"batch_size": 32
	},
	"model": {
		"loss": "mse",
		"optimizer": "adam",
		"save_dir": "saved_models",
		"layers": [
			{
				"type": "lstm",
				"neurons": 100,
				"input_timesteps": 20,
				"input_dim": 2,
				"return_seq": true
			},
			{
				"type": "dropout",
				"rate": 0.2
			},
			{
				"type": "lstm",
				"neurons": 100,
				"return_seq": true
			},
			{
				"type": "lstm",
				"neurons": 100,
				"return_seq": false
			},
			{
				"type": "dropout",
				"rate": 0.2
			},
			{
				"type": "dense",
				"neurons": 1,
				"activation": "linear"
			}
		]
	}
}
