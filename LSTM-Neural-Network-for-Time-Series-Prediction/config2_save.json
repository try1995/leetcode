{
	"data": {
		"filename": "my.csv",
		"columns": ["Close", "Volume", "Money","BIAS", "BOLL", "DMI_ADX", "DMI_ADXR", "EXPMA", "KDJ_K", "KDJ_D", "KDJ_J", "RSI", "MA", "MACD"],
		"sequence_length": 20,
		"test_data_num": 20,
		"normalise": true,
		"drop": true
	},
	"training": {
		"epochs": 2,
		"batch_size": 32
	},
	"model": {
		"loss": "mse",
		"optimizer": "adam",
		"save_dir": "saved_models",
		"layers": [
			{
				"type": "lstm",
				"neurons": 1024,
				"input_timesteps": 19,
				"input_dim": 14,
				"return_seq": true
			},
			{
				"type": "dropout",
				"rate": 0.05
			},
			{
				"type": "lstm",
				"neurons": 512,
				"return_seq": true
			},
			{
				"type": "dropout",
				"rate": 0.05
			},
			{
				"type": "lstm",
				"neurons": 256,
				"return_seq": false
			},
			{
				"type": "dropout",
				"rate": 0.05
			},
			{
				"type": "dense",
				"neurons": 128,
				"activation": "linear"
			},
			{
				"type": "dense",
				"neurons": 6,
				"activation": "linear"
			},
			{
				"type": "dense",
				"neurons": 1,
				"activation": "linear"
			}
		]
	}
}
